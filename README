COMPILE

old mysql
gcc -o galaxc $(mysql_config --cflags) mysql.c $(mysql_config --libs)

old wand
cc `MagickWand-config --cflags --cppflags` -o wand wand.c `MagickWand-config --ldflags --libs`

new galaxc
gcc -o galaxc $(MagickWand-config --cflags --cppflags) $(mysql_config --cflags) main.c mysql.c wand.c $(mysql_config --libs) `MagickWand-config --ldflags --libs`
